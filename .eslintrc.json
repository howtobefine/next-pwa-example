{
  "extends": "next/core-web-vitals",
  "rules": {
    "react/display-name": "off",
    "@typescript-eslint/no-explicit-any": "off",
    "react-refresh/only-export-components": "off",
    "jsx-a11y/alt-text": "off",
    "react-hooks/exhaustive-deps": 0,
    "indent": ["error", 2],
    "quotes": ["error", "double"],
    "semi": ["error", "never"]
  }
}
